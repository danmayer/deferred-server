<h1>Project: <a href="/<%= @project_key %>"><%= @project_key %></a></h1>
<h3>commit: <%= @commit %></h3>

<% if @commit_hash %>
  <p>
    <strong>commit message:</strong> <%= @commit_hash['commits'][0]['message'] %>
  </p>
<% end %>

<% if @run_results['cmd_run'] %>
  <p>
    <strong>command run:</strong> <code><%= @run_results['cmd_run'] %></code>
  </p>
<% end %>

<% if @run_results['exit_status'] %>
  <p>
    <strong>command exit status:</strong> <code><%= @run_results['exit_status'] %></code>
  </p>
<% end %>

<pre>
  <%= @run_results['results'] %>
</pre>